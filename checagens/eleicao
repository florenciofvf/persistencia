<set>
    <![CDATA[
        senao($DESCRICAO='SEG', 'SEGUNDA-FEIRA',
               senao($DESCRICAO='TER', 'TERCA-FEIRA',
                      senao($DESCRICAO='QUA', 'QUARTA-FEIRA',
                             senao($DESCRICAO='QUI', 'QUINTA-FEIRA',
                                    senao($DESCRICAO='SEX', 'SEXTA-FEIRA',
                                           senao(($DESCRICAO='SAB') || ($DESCRICAO='DOM'), 
                                                 'FINAL DE SEMANA', 'DIA INVÁLIDO'
                                           )
                                    )
                             )
                      )
               )
        )
    ]]>
</set>
<set>
    <![CDATA[
        switch(
              $DESCRICAO='SEG', 'SEGUNDA-FEIRA',
              $DESCRICAO='TER', 'TERCA-FEIRA',
              $DESCRICAO='QUA', 'QUARTA-FEIRA',
              $DESCRICAO='QUI', 'QUINTA-FEIRA',
              $DESCRICAO='SEX', 'SEXTA-FEIRA',
              ($DESCRICAO='SAB') || ($DESCRICAO='DOM'), 'FINAL DE SEMANA', 
              'DIA INVÁLIDO'
        )
    ]]>
</set>
<set>
    <![CDATA[
        isEmpty($DATA_INICIADA)
    ]]>
</set>
<set>
    <![CDATA[
        $DATA_INICIADA
    ]]>
</set>
<set>
    <![CDATA[
        returnNull(
            set('connEleicao',
                getConnection('com.mysql.cj.jdbc.Driver','jdbc:mysql://localhost/eleicao','root','***')
            )
        )
    ]]>
</set>
<set>
    <![CDATA[
        returnNull(
            set('listaIdCandidato',
                select($connEleicao, 'select id from candidato2 where eleicao_id = idEleicao', 'idEleicao', $ID)
            )
        )
    ]]>
</set>
<set>
    <![CDATA[
        ifElse(isEmpty($listaIdCandidato), 'SEM CANDIDATO2', 'CONTÉM CANDIDATO2')
    ]]>
</set>
<set>
    <![CDATA[
        closeConnection($connEleicao)
    ]]>
</set>
